<%= form_with(url: "#", local: true, method: :get, class: "search-form") do |f| %>
  <%# <%= f.collection_select(:hoge, [], :hoge, :hoge, {}, {class:"select-box", id:"erea-category"}) %> 
  <%= f.text_field :keyword, placeholder: "投稿を検索する", class: "search-input" %>
  <%= f.submit "検索", class: "search-btn" %>
<% end %>
<div class="contents row">
  <%# 投稿の表示 %>
  <% 10.times do %> <%#  後で変える %>
    <%= link_to "#" do %>
    <div class="content_post" >
    <%= image_tag 'sample.jpg', class: "item-img" %>
      <p><%= "(都道府県名)" %></p>
      <span class="name">
          <span>投稿者</span><%= "(ユーザー名)" %>
      </span>
    </div>
    <div class="content_bottom">
      <div class="travel-title"><%= "(旅先名)" %></div>
      <div class="star-rating mb-2">
        <div class="star-rating-front" style="width: <%= "#" %>%">★★★★★</div>
        <div class="star-rating-back">★★★★★</div>
      </div>
    </div>
    <% end %>
    
  <% end %>
</div>